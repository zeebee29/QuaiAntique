<body>
<form id="dataResa" data1={{plagesCompletes}} data2={{hebdo}} data3={{plagesH}}></form>
    <section class="ftco-section">
        <div class="container-resa">
            <div class="row content-center">
                <div class="col-md-6 mb-2">
                    {% if nbPers > 1 %}
                    <h2 class="titre-page">Réservation pour {{nbPers}} personnes</h2>
                    {% else %}
                    <h2 class="titre-page">Réservation pour {{nbPers}} personne</h2>
                    {% endif %}

                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="content w-100">
                        <div class="calendar-container">
                            <div class="calendar">
                                <div class="year-line">
                                    <span id="prev-year" class="left-button fa fa-chevron-left"> </span>
                                    <span id="year" class="year"></span>
                                    <span id="next-year" class="right-button fa fa-chevron-right"> </span>
                                </div>
                                <table id="month-list" class="months-table w-100">
                                    <tbody>
                                        <tr class="months-row">
                                            <td class="month">Jan</td>
                                            <td class="month">Fev</td>
                                            <td class="month">Mar</td>
                                            <td class="month">Avr</td>
                                            <td class="month">Mai</td>
                                            <td class="month">Juin</td>
                                            <td class="month">Juil</td>
                                            <td class="month">Aout</td>
                                            <td class="month">Sept</td>
                                            <td class="month">Oct</td>
                                            <td class="month">Nov</td>
                                            <td class="month">Dec</td>
                                        </tr>
                                    </tbody>
                                </table>

                                <table class="days-table w-100">
                                    <td class="day">Lun</td>
                                    <td class="day">Mar</td>
                                    <td class="day">Mer</td>
                                    <td class="day">Jeu</td>
                                    <td class="day">Ven</td>
                                    <td class="day">Sam</td>
                                    <td class="day">Dim</td>
                                </table>
                                <div class="frame">
                                    <table class="dates-table w-100">
                                        <tbody id="tbody" class="tbody">
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
						<div id="slots-cont" class="slots-container">
                            <div id="slot-card" class="slot-card"></div>
                            <div id="date-slot" class="date-slot"></div>
							<div id="slots-part1" class="slots">
								<div id="sstitre1" class="titre-plage">
									<h3></h3>
								</div>
                                <div id="msg-slot1" class="msg-slot"></div>
								<table id="tb1-plages" class="tb-plages">
								</table>
							</div>
							<div id="slots-part2" class="slots">
								<div id="sstitre2" class="titre-plage">
									<h3></h3>
								</div>
                                <div id="msg-slot2" class="msg-slot"></div>
								<table id="tb2-plages" class="tb-plages">
								</table>
							</div>
						</div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</form>

</body>